{
  "name": "reactnativeauthdashboard",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "bash -c 'react-native run-ios --simulator=\"iPhone 14 Pro Max\"'",
    "newclear": "rm -rf node_modules && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/haste-map-react-native-packager-* && rm -rf $TMPDIR/metro-bundler-cache-* && watchman watch-del-all && rm -rf ios/build && npm cache clean --force && npm i && cd ios && rm -rf Pods && pod install && cd .. && npm start -- --reset-cache",
    "start": "react-native start",
    "test": "jest",
    "postinstall": "patch-package",
    "lint": "eslint .",
    "storybook": "start-storybook -p 7007",
    "story:load": "rnstl"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@react-native-async-storage/async-storage": "^1.21.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/stack": "^6.3.20",
    "react": "17.0.2",
    "react-native": "0.68.1",
    "react-native-device-info": "^10.12.0",
    "react-native-gesture-handler": "^2.14.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-safe-area-context": "4.3.4",
    "react-native-screens": "^3.29.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@storybook/addon-actions": "^5.3.21",
    "@storybook/addon-knobs": "^5.3.21",
    "@storybook/addon-links": "^5.3.21",
    "@storybook/addon-ondevice-actions": "^5.3.23",
    "@storybook/addon-ondevice-knobs": "^5.3.25",
    "@storybook/react-native": "^5.3.18",
    "@storybook/react-native-server": "^5.3.23",
    "@tsconfig/react-native": "^3.0.2",
    "@types/jest": "^29.5.11",
    "@types/react": "^18.2.43",
    "@types/react-test-renderer": "^18.0.7",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.5",
    "babel-plugin-inline-import": "^3.0.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "mobx": "^6.5.0",
    "mobx-react": "^7.3.0",
    "mobx-state-tree": "^5.1.7",
    "mobx-state-tree-persist": "^0.2.0",
    "metro-react-native-babel-preset": "^0.67.0",
    "patch-package": "^8.0.0",
    "react-native-storybook-loader": "^2.0.5",
    "react-test-renderer": "17.0.2",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "config": {
    "react-native-storybook-loader": {
      "searchDir": "./src",
      "pattern": "**/*.story.tsx",
      "outputFile": "./storybook/storyLoader.js"
    }
  }
}
