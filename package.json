{
  "name": "chatapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "android": "bash -c 'ENVFILE=.env.${1-development} react-native run-android' -- ",
    "android:clean": "bash -c 'cd android && ./gradlew clean && cd .. && ENVFILE=.env.${1-development} react-native run-android' -- ",
    "ios": "bash -c 'ENVFILE=.env.${1-development} react-native run-ios --simulator=\"iPhone 14 Pro Max\"' -- ",
    "ios:personal": "bash -c 'ENVFILE=.env.${1-development} react-native run-ios --device=\"Greenan\"' -- ",
    "start": "react-native start",
    "newclear": "rm -rf node_modules && rm -rf $TMPDIR/react-* && rm -rf $TMPDIR/haste-map-react-native-packager-* && rm -rf $TMPDIR/metro-bundler-cache-* && watchman watch-del-all && rm -rf ios/build && npm cache clean --force && npm i && cd ios && rm -rf Pods && pod install && cd .. && npm start -- --reset-cache",
    "story:load": "rnstl",
    "test": "NODE_ENV=test TZ=UTC jest",
    "test:watch": "NODE_ENV=test TZ=UTC jest --watch",
    "storybook": "start-storybook -p 7007",
    "build-storybook": "build-storybook",
    "postinstall": "patch-package",
    "types": "tsc -p tsconfig.json --noEmit"
  },
  "dependencies": {
    "@babel/plugin-proposal-decorators": "^7.17.9",
    "@babel/plugin-transform-runtime": "^7.17.10",
    "@react-native-async-storage/async-storage": "^1.18.1",
